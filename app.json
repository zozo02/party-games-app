{
  "expo": {
    "name": "PartyGames",
    "slug": "party-games",
    "scheme": "partygames",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0b12"
    },
    "owner": null,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.partygames",
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.example.partygames",
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0b12"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    }
  }
}